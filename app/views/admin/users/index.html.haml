%h1 Users

%table
  - @users.each do |user|
    %tr
      %td= user.email
      %td
        = link_to 'show', admin_user_path(user)
        = link_to 'edit', edit_admin_user_path(user)
        = link_to 'delete', admin_user_path(user), method: :delete, data: {confirm: "Are you sure you want to delete this user?"}

%p= link_to 'create a new user', new_admin_user_path
